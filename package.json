{
  "devDependencies": {
    "husky": "3.0.2",
    "prettier": "1.18.2",
    "prettier-plugin-java": "0.8.0",
    "pretty-quick": "1.11.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern '**/*.java' --pattern '**/*.json' --pattern '**/*.md' --pattern '**/*.yml'"
    }
  }
}
